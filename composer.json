{
    "name": "robwittman/laravel-shopify-plugin",
    "description": "Library to integrate Shopify with Laravel",
    "authors": [
        {
            "name": "Rob Wittman",
            "email": "rob@ihsdigital.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.5",
        "robwittman/shopify-php-sdk": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelShopifyPlugin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelShopifyPlugin\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelShopifyPlugin\\Providers\\ServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    }
}
